{
  "name": "vigour-env",
  "version": "1.0.3",
  "description": "Provides info about the device, platform and build",
  "main": "lib/index.js",
  "scripts": {
    "build": "cd example && npm run build",
    "ios": "cd example && npm run ios",
    "android": "cd example && npm run android",
    "all": "cd example && npm run all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/env.git"
  },
  "keywords": [
    "vigour",
    "wrapper",
    "native",
    "plugin",
    "env"
  ],
  "author": "Shawn Inder <shawn@vigour.io>",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vigour-io/env/issues"
  },
  "homepage": "https://github.com/vigour-io/env",
  "engines": {
    "node": ">=0.12"
  },
  "dependencies": {
    "npmlog": "^1.2.1",
    "promise": "^7.0.4",
    "vigour-wrapper": "^2.1.1"
  },
  "vigour": {
    "plugin": {
      "android": {
        "className": "io.vigour.plugin.env.EnvPlugin",
        "instantiation": "new EnvPlugin(this)"
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "sinon"
    ]
  }
}
